{
  "env": {
    "jquery": true,
    "browser": true
  },
  "globals": {
    "feather": "readonly",
    "tinysort": "readonly"
  },
  "extends": [
    "eslint:recommended",
    "jquery"
  ],
  "rules": {
    "indent": [
      "error",
      2
    ],
    "linebreak-style": [
      "error",
      "unix"
    ],
    "strict": 1
  }
}
